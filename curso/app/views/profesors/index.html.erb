<h1>Listado de profesores</h1>

<table>
  <tr>
	<th>ID_Profesor</th>
    <th>Nombre</th>
    <th></th>
  </tr>

  <% @profesors.each do |profesor| %>
    <tr>
      <td><%= profesor.id %></td>
      <td><%= profesor.nombre %></td>
      <td><%= link_to 'Ver', profesor_path(profesor) %></td>
      <td><%= link_to 'Editar', edit_profesor_path(profesor) %></td>
      <td><%= link_to 'Eliminar', profesor_path(profesor),
              method: :delete,
              data: { confirm: '¿Está seguro?' } %></td>
    </tr>
  <% end %>
</table>
<br/>
<%= link_to 'Agregar nuevo profesor', new_profesor_path %> | 
<%= link_to "Volver al inicio", root_path %>
